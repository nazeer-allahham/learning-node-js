{
    "compileOnSave": false,
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "moduleResolution": "node",
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "noUnusedLocals": true,
      "sourceMap": true,
      "declaration": false,
      "pretty": true,
      "strictNullChecks": true,
      "strictPropertyInitialization": true
    },
    "include": [
      "**/**/*.ts",
      "app.ts"
    ],
    "exclude": [
      "node_modules"
    ]
  }
  